<?xml version="1.0"?>
<launch>

  <!-- send cup to param server -->
	<param name="kitchen" textfile="/home/aub/ipa-kitchen.urdf" />

	<!-- spawn model in gazebo -->
	<node name="spawn_ipa_kitchen" pkg="gazebo" type="spawn_model" args="-urdf -param kitchen -x 0.0 -y -0.0 -z .01  -model ipa-kitchen" respawn="false" output="screen" />

	<!-- cup in kitchen 
	<include file="$(find cob_gazebo_worlds)/ros/launch/china_cup.launch" />-->
	
	<!-- cup on table 
	<include file="$(find cob_gazebo_worlds)/ros/launch/coffee_cup_table.launch" />-->
	
	<!-- table 
	<include file="$(find cob_gazebo_worlds)/ros/launch/table.launch" />-->

</launch>
